<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Customize your theme here. -->

<!--  Acee  98ee99   prim 689f38-->
        <item name="colorPrimary">#387002</item>
        <item name="colorPrimaryDark">#316101</item>
        <item name="colorAccent">#55BC56</item>
        <item name="colorSecondary">#66bb6a</item>
    </style>

    <style name="splash" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#CD000000</item>
        <item name="colorPrimaryDark">#CD000000</item>
        <item name="android:windowTranslucentStatus">true</item>
        <item name="android:windowTranslucentNavigation">true</item>
        <item name="colorAccent">#98ee99</item>
        <item name="colorSecondary">#66bb6a</item>
    </style>
<!--    Theme.AppCompat.Light.DarkActionBar-->
    <style name="add_update" parent="Theme.AppCompat.Light.DarkActionBar">
        <item name="colorPrimary">#000000</item>
        <item name="colorPrimaryDark">#000000</item>xx
        <item name="colorAccent">#c60055</item>
        <item name="colorSecondary">#bdbdbd</item>
    </style>
<!--    below are the background colors of add activity
        add activity: EE000000   app bar(color primary) color: EE040404 -->

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    
    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>

    <style name="expandedBarTestStyle" parent="Theme.AppCompat.NoActionBar">
        <item name="android:textSize">0sp</item>
        <item name="android:textColor">@android:color/transparent</item>
    </style>

    <style name="viewUserStyle" parent="Theme.AppCompat.NoActionBar">
        <item name="colorPrimary">#387002</item>
        <item name="colorPrimaryDark">#316101</item>
        <item name="colorAccent">#55BC56</item>
        <item name="colorSecondary">#66bb6a</item>
        <item name="actionModeBackground">@color/action_mode_color</item>
        <item name="windowActionModeOverlay">true</item>
    </style>



</resources>